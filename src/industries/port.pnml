switch(FEAT_INDUSTRIES, SELF, port_switch_location_check_industry_distance,
    [STORE_TEMP(16, TEMP_REGISTER_TOWN_MIN_DISTANCE),
	 STORE_TEMP(64, TEMP_REGISTER_TOWN_MAX_DISTANCE),
	 count_industries(),
	   (is_industry_funded_or_prospected()) || (distance_from_town() && 
	   (is_near_map_edge(32)) &&
	   (param_max_industries_per_type_on_map == 0 || param_max_industries_per_type_on_map > industry_count(INDUSTRY_ID_PORT)) &&
	   (param_max_industries_on_map == 0 || param_max_industries_on_map > LOAD_TEMP(TEMP_REGISTER_NUMBER_OF_INDUSTRIES)) &&
	   // distance to related producers
	   (industry_distance(INDUSTRY_ID_APPLIANCE_FACTORY) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_CLEANING_PRODUCTS_FACTORY) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_CLOTHING_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_FURNITURE_FACTORY) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_PHARMACEUTICAL_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_PRINTING_WORKS) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_VEHICLE_FACTORY) >= param_min_dist_related_industry) &&
	   // distance to related consumers
	   (industry_distance(INDUSTRY_ID_ALUMINIUM_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_BRICK_WORKS) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_CARBON_BLACK_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_COAL_LIQUEFACTION_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_COKE_OVEN) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_COPPER_SMELTER) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_INTEGRATED_STEEL_MILL) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_OIL_REFINERY) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_ORE_SMELTER) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_PAINT_FACTORY) >= param_min_dist_related_industry) && 
	   (industry_distance(INDUSTRY_ID_PLASTICS_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_POWER_PLANT) >= param_min_dist_related_industry) &&
	   (industry_distance(INDUSTRY_ID_TYRE_PLANT) >= param_min_dist_related_industry) &&	   
	   // distance between instances of same industry
	   (industry_distance(INDUSTRY_ID_PORT) >= param_min_dist_same_industry)	   
	   )])
{
	   1: return CB_RESULT_LOCATION_ALLOW;
	   return CB_RESULT_LOCATION_DISALLOW;
}

