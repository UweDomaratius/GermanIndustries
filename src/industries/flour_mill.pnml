item(FEAT_INDUSTRYTILES, flour_mill_tile_1, INDUSTRY_TILE_ID_FLOUR_MILL) {
	if (param_extension_food_industries && !param_extension_packaging_industries) {
		property {
			substitute:         0;
			land_shape_flags:   0;
			accepted_cargos: [[GRAI,8]];
			animation_info: [ANIMATION_LOOPING, 6];
			animation_speed: 3;
			animation_triggers: bitmask();
		}
	}
	if (param_extension_food_industries && param_extension_packaging_industries) {
		property {
			substitute:         0;
			land_shape_flags:   0;
			accepted_cargos: [[GRAI,8],[MNSP,8]];
			animation_info: [ANIMATION_LOOPING, 6];
			animation_speed: 3;
			animation_triggers: bitmask();
		}
	}
	graphics {
		tile_check: flour_mill_switch_tile_location_check;
		cargo_amount_accept: flour_mill_switch_tile_cargo_acceptance;
		flour_mill_switch_graphics;
	}
}

