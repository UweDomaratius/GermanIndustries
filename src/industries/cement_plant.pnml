switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_location_check_industry_distance,
	[STORE_TEMP(32, TEMP_REGISTER_TOWN_MIN_DISTANCE),
	 STORE_TEMP(64, TEMP_REGISTER_TOWN_MAX_DISTANCE),
	 count_industries(),
	 (is_industry_funded_or_prospected()) || (distance_from_town() && 
	 (param_max_industries_per_type_on_map == 0 || param_max_industries_per_type_on_map > industry_count(INDUSTRY_ID_CEMENT_PLANT)) &&
	 (param_max_industries_on_map == 0 || param_max_industries_on_map > LOAD_TEMP(TEMP_REGISTER_NUMBER_OF_INDUSTRIES)) &&
	 // distance to related producers
	 (industry_distance(INDUSTRY_ID_LIMESTONE_MINE) >= param_min_dist_related_industry) &&
	 (industry_distance(INDUSTRY_ID_SANDPIT) >= param_min_dist_related_industry) && 	 
	 // distance to related consumers     
	 (industry_distance(INDUSTRY_ID_BUILDERS_YARD) >= param_min_dist_related_industry) &&
	 // distance between instances of same industry
	 (industry_distance(INDUSTRY_ID_CEMENT_PLANT) >= param_min_dist_same_industry)
	)]) {
	   1: return CB_RESULT_LOCATION_ALLOW;
	   return CB_RESULT_LOCATION_DISALLOW;
}

// 1 SAND + 1 LIMESTONE => 0.63 CEMENT
produce(cement_plant_produce,
	[SAND: LOAD_TEMP(TEMP_REGISTER_PRODUCTION_AMOUNT);
	 LIME: LOAD_TEMP(TEMP_REGISTER_PRODUCTION_AMOUNT);],
	[CMNT: LOAD_TEMP(TEMP_REGISTER_PRODUCT_1);
	],
	0 // do not run again
)

// industry uses 10% of stockpile for production, if stockpile is big enough
// if stockpile is < 80t, 8t are produced per 256ticks
// if stockpile is < 8t, whatever is available is used
// it stockpile is empty, nothing is produced
switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_produce, 
	[STORE_TEMP(min(incoming_cargo_waiting("SAND"),incoming_cargo_waiting("LIME")),TEMP_REGISTER_INCOMING_CARGO_WAITING0),
     
	 calculate_raw_material_usage(),
	 calculate_total_production(),
	 STORE_TEMP(percentage_of_production(63), TEMP_REGISTER_PRODUCT_1),

     store_debug_production_data()	 
	]) {
	cement_plant_produce;
}

// only show stockpile for SAND, LIME and nothing else
switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_cargo_subtype, getbits(extra_callback_info2, 16, 8)) {
	SAND: switch_cargo_subtype;
	LIME: switch_cargo_subtype;
	return 0x3800 + string(STR_EMPTY);
}

switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_extra_text_fund, 
	[STORE_TEMP(string(STR_1800), TEMP_REGISTER_EXTRA_TEXT_ARG0),
	current_year]) {
	1800..500000: return string(STR_EMPTY);
	return string(STR_INDUSTRY_NOT_AVAILABLE_BEFORE);
}

// year < 1800: no creation, 1800 <= year: certain probability
switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_check_availability, 
	current_year) {
	0..1799: return CB_RESULT_IND_NO_CONSTRUCTION;
	get_construction_probability;
}

switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_update_last_served_date,
	[update_last_served_date(),
	 STORE_PERM(this_month_production("CMNT") != 0 || last_month_production("CMNT") != 0 || incoming_cargo_waiting("SAND") != 0 || incoming_cargo_waiting("LIME") != 0 || this_month_transported("CMNT") != 0 || last_month_transported("CMNT") != 0, PERM_REGISTER_LAST_SERVED11)
	])
{
	check_secondary_industry_closure();
}

produce(cement_plant_produce_cap_stockpile,
	[SAND: incoming_cargo_waiting("SAND") > 60000 ? incoming_cargo_waiting("SAND") - 60000 : 0;
	 LIME: incoming_cargo_waiting("LIME") > 60000 ? incoming_cargo_waiting("LIME") - 60000 : 0;
	 ],
	[],
	0 // do not run again
)

switch(FEAT_INDUSTRIES, SELF, cement_plant_switch_stockpile_check, param_stockpile_cap)
{
	1: empty_produce;
	cement_plant_produce_cap_stockpile;
}

if (param_extension_building_materials) {
	item(FEAT_INDUSTRIES, cement_plant, INDUSTRY_ID_CEMENT_PLANT) {
		property {
			substitute: 0;
			map_colour: 19;
			life_type: IND_LIFE_TYPE_PROCESSING;
			min_cargo_distr: 1;
			layouts: [cement_plant_tilelayout_1,cement_plant_tilelayout_2,cement_plant_tilelayout_3,cement_plant_tilelayout_4,cement_plant_tilelayout_5];
			spec_flags: bitmask(IND_FLAG_LONG_CARGO_TYPE_LISTS);
			conflicting_ind_types: [];
			random_sound_effects: [];
			name: string(STR_IND_CEMENT_PLANT);
			prob_map_gen: 5;
			prob_in_game: 3;
			fund_cost_multiplier: 203;
			remove_cost_multiplier: 0;
			remove_cost_multiplier: 0;
			cargo_types: [accept_cargo("SAND"),accept_cargo("LIME"),produce_cargo("CMNT",0)];
			nearby_station_name: string(STR_STATION, string(STR_TOWN),string(STR_STATION_CEMENT_PLANT));
		}
			
		graphics {
			construction_probability: cement_plant_switch_check_availability;
			produce_cargo_arrival:    cement_plant_switch_stockpile_check; 
			produce_256_ticks:        cement_plant_switch_produce;
			monthly_prod_change:      cement_plant_switch_update_last_served_date;
			random_prod_change:       return CB_RESULT_IND_PROD_NO_CHANGE;
			location_check:           cement_plant_switch_location_check_industry_distance;
			extra_text_fund:          cement_plant_switch_extra_text_fund;
			cargo_subtype_display:    cement_plant_switch_cargo_subtype;
			build_prod_change:        initialize_last_served_date;
			stop_accept_cargo:        switch_stop_accept_cargo;
		}
	}
} // param_extension_building_materials
    