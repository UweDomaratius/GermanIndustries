	[update_last_served_date(),
	 STORE_PERM(this_month_production("STSH") != 0 || last_month_production("STSH") != 0 || 
		incoming_cargo_waiting("COPR") != 0 || incoming_cargo_waiting("ACID") != 0 || incoming_cargo_waiting("LYE_") != 0 ||
		this_month_transported("STSH") != 0 || last_month_transported("STSH") != 0, PERM_REGISTER_LAST_SERVED11)
	])
{
	check_secondary_industry_closure();
}

