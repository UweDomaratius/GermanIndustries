if (param_extension_metallurgy && !param_extension_aluminium && !param_extension_recycling) {
	item(FEAT_INDUSTRIES, wire_mill, INDUSTRY_ID_WIRE_MILL) {
		property {
			substitute: 0;
			map_colour: 107;
			life_type: IND_LIFE_TYPE_PROCESSING;
			min_cargo_distr: 1;
			layouts: [wire_mill_tilelayout_1,wire_mill_tilelayout_2,wire_mill_tilelayout_3,wire_mill_tilelayout_4,wire_mill_tilelayout_5,wire_mill_tilelayout_6,wire_mill_tilelayout_7,wire_mill_tilelayout_8];
			spec_flags: bitmask(IND_FLAG_LONG_CARGO_TYPE_LISTS);
			conflicting_ind_types: [];
			random_sound_effects: [];
			name: string(STR_IND_WIRE_MILL);
			prob_map_gen: 5;
			prob_in_game: 3;
			fund_cost_multiplier: 190;
			remove_cost_multiplier: 0;
			remove_cost_multiplier: 0;
			cargo_types: [accept_cargo("COPR"),accept_cargo("ACID"),accept_cargo("LYE_"),produce_cargo("STWR",0)];
			nearby_station_name: string(STR_STATION, string(STR_TOWN),string(STR_STATION_WIRE_MILL));
		}
			
		graphics {
			construction_probability: wire_mill_switch_check_availability;
			produce_cargo_arrival:    empty_produce; 
			produce_256_ticks:        wire_mill_switch_produce;
			monthly_prod_change:      wire_mill_switch_update_last_served_date;
			random_prod_change:       return CB_RESULT_IND_PROD_NO_CHANGE;
			location_check:           wire_mill_switch_location_check_industry_distance;
			extra_text_fund:          wire_mill_switch_extra_text_fund;
