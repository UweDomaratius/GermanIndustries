	relative_coord(0, 0): explosives_factory_spritelayout_tall_building_1;
	relative_coord(0, 1): explosives_factory_spritelayout_tall_building_1;
	relative_coord(0, 2): explosives_factory_spritelayout_paper_store_full;
	relative_coord(1, 0): explosives_factory_spritelayout_tall_building_2;
	relative_coord(1, 1): explosives_factory_spritelayout_pulp_processor;
	relative_coord(1, 2): explosives_factory_spritelayout_paper_store_empty;
	relative_coord(2, 0): explosives_factory_spritelayout_tall_building_1;
	relative_coord(2, 1): explosives_factory_spritelayout_chemical_tanks;
	relative_coord(2, 2): explosives_factory_spritelayout_paper_store_full;
	relative_coord(3, 0): explosives_factory_spritelayout_tall_building_2;
	relative_coord(3, 1): explosives_factory_spritelayout_wood_store_forklift;
	relative_coord(3, 2): explosives_factory_spritelayout_wood_store_full;
	relative_coord(4, 0): explosives_factory_spritelayout_boilerhouse;
	relative_coord(4, 1): explosives_factory_spritelayout_wood_store_full;
	relative_coord(4, 2): explosives_factory_spritelayout_wood_store_forklift;
	return 0;
