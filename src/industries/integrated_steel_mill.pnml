switch(FEAT_INDUSTRIES, SELF, steel_mill_switch_location_check_industry_distance,
       [STORE_TEMP(32, TEMP_REGISTER_TOWN_MIN_DISTANCE),
	    STORE_TEMP(64, TEMP_REGISTER_TOWN_MAX_DISTANCE),
		count_industries(),
	    (is_industry_funded_or_prospected()) || (distance_from_town() && 
		(param_max_industries_per_type_on_map == 0 || param_max_industries_per_type_on_map > industry_count(INDUSTRY_ID_INTEGRATED_STEEL_MILL)) &&
		(param_max_industries_on_map == 0 || param_max_industries_on_map > LOAD_TEMP(TEMP_REGISTER_NUMBER_OF_INDUSTRIES)) &&
		// distance to related producers
	    (industry_distance(INDUSTRY_ID_COAL_MINE) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_COKE_OVEN) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_CRYO_PLANT) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_FOUNDRY_FORGE) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_IRON_ORE_MINE) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_LIME_KILN) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_ORE_SMELTER) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_PORT) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_SCRAP_YARD) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_SHEET_MILL) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_WIRE_MILL) >= param_min_dist_related_industry) && 
		// distance to related consumers
		(industry_distance(INDUSTRY_ID_VEHICLE_BODY_PLANT) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_VEHICLE_ENGINE_PLANT) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_VEHICLE_FACTORY) >= param_min_dist_related_industry) && 
		// distance between instances of same industry
	    (industry_distance(INDUSTRY_ID_INTEGRATED_STEEL_MILL) >= param_min_dist_same_industry)
		
		)]) {
	   1: return CB_RESULT_LOCATION_ALLOW;
	   return CB_RESULT_LOCATION_DISALLOW;
}

