switch(FEAT_INDUSTRIES, SELF, coke_oven_switch_location_check_industry_distance,
       [STORE_TEMP(32, TEMP_REGISTER_TOWN_MIN_DISTANCE),
	    STORE_TEMP(64, TEMP_REGISTER_TOWN_MAX_DISTANCE),
		count_industries(),
	    (is_industry_funded_or_prospected()) || ((distance_from_town() && 
		(param_max_industries_per_type_on_map == 0 || param_max_industries_per_type_on_map > industry_count(INDUSTRY_ID_COKE_OVEN)) &&
		(param_max_industries_on_map == 0 || param_max_industries_on_map > LOAD_TEMP(TEMP_REGISTER_NUMBER_OF_INDUSTRIES)) &&
		// distance to related producers
		(industry_distance(INDUSTRY_ID_COAL_MINE) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_PORT) >= param_min_dist_related_industry) && 
		// distance to related consumers
	    (industry_distance(INDUSTRY_ID_ACID_PLANT) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_FOUNDRY_FORGE) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_INTEGRATED_STEEL_MILL) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_LIME_KILN) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_ORE_SMELTER) >= param_min_dist_related_industry) && 
		(industry_distance(INDUSTRY_ID_TYRE_PLANT) >= param_min_dist_related_industry) &&		
		// distance between instances of same industry
	    (industry_distance(INDUSTRY_ID_COKE_OVEN) >= param_min_dist_same_industry))		
		)]) {
	   1: return CB_RESULT_LOCATION_ALLOW;
	   return CB_RESULT_LOCATION_DISALLOW;
}

// acording to google, one needs 1.37t coal to produce 1t of coke
// 1t coal contains about 10kg sulphur (1%), this is rounded up here for the production step
produce(coke_oven_produce,
	[COAL: LOAD_TEMP(TEMP_REGISTER_PRODUCTION_AMOUNT);
	 ],
	[COKE: LOAD_TEMP(TEMP_REGISTER_PRODUCT_1);
	 SULP: LOAD_TEMP(TEMP_REGISTER_PRODUCT_2);
	],
	0 // do not run again
)

switch(FEAT_INDUSTRIES, SELF, coke_oven_switch_produce, 
	[STORE_TEMP(incoming_cargo_waiting("COAL"), TEMP_REGISTER_INCOMING_CARGO_WAITING0),
	 
	 calculate_raw_material_usage(),
	 calculate_total_production(),

	 store_debug_production_data(),

	 STORE_TEMP(percentage_of_production(73), TEMP_REGISTER_PRODUCT_1), // COKE
	 STORE_TEMP(percentage_of_production(5), TEMP_REGISTER_PRODUCT_2),  // SULP
	]) {
	coke_oven_produce;
}

switch(FEAT_INDUSTRIES, SELF, coke_oven_switch_cargo_subtype, getbits(extra_callback_info2, 16, 8)) {
	COAL: switch_cargo_subtype;
	return 0x3800 + string(STR_EMPTY);
}

switch(FEAT_INDUSTRIES, SELF, coke_oven_switch_update_last_served_date,
	[update_last_served_date(),
	 STORE_PERM(this_month_production("COKE") != 0 || last_month_production("COKE") != 0 || 
		incoming_cargo_waiting("COAL") != 0 ||
		this_month_transported("COKE") != 0 || last_month_transported("COKE") != 0, PERM_REGISTER_LAST_SERVED11)
	])
{
	check_secondary_industry_closure();
}

switch(FEAT_INDUSTRIES, SELF, coke_oven_switch_extra_text_fund, 
	[STORE_TEMP(string(STR_1800), TEMP_REGISTER_EXTRA_TEXT_ARG0),
	current_year]) {
	1800..500000: return string(STR_EMPTY);
	return string(STR_INDUSTRY_NOT_AVAILABLE_BEFORE);
}

switch(FEAT_INDUSTRIES, SELF, coke_oven_switch_check_availability, 
	current_year) {
	0..1799: return CB_RESULT_IND_NO_CONSTRUCTION;
	get_construction_probability;
}

produce(coke_oven_produce_cap_stockpile,
	[COAL: incoming_cargo_waiting("COAL") > 60000 ? incoming_cargo_waiting("COAL") - 60000 : 0;
