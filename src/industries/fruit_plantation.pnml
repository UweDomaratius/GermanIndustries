spritelayout fruit_plantation_house_spritelayout {
	childsprite {
		sprite: TTD_GROUND_SPRITE_MOSTLY_GREEN;
		hide_sprite: 0;
		always_draw: 1;
	}
	building {
		sprite: fruit_plantation_spriteset_1(0);
		recolour_mode: RECOLOUR_REMAP;
		palette: PALETTE_USE_DEFAULT;
		xoffset: 0;
		yoffset: 0;
		zoffset: 0;
		xextent: 16;
		yextent: 16;
		zextent: 32;
		always_draw: 0;
		hide_sprite: (construction_state == 0 || LOAD_TEMP(TEMP_REGISTER_HIDE_TILE));
	}
}
